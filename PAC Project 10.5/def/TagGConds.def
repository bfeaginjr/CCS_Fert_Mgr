    #define  BOARD_SMART_DIGITAL_SNAP         0x00002000
    #define  BOARD_SMART_ANALOG_SNAP          0x00004000
    #define  BOARD_SMART_DIGITAL_B100         0x00010000
    #define  BOARD_REMOTE_SIMPLE_DIGITAL_SNAP 0X00020000 
    #define  BOARD_MIXED_SNAP_64              0x00080000 ; */


